    function addNewData() {

      // const newImage = document.querySelector("#dialog_edit input[name='studentImage']").value;
      const newMssv = document.querySelector("#dialog_edit input[name='mssv']").value;
      const newPhone = document.querySelector("#dialog_edit input[name='phone']").value;
      const newName = document.querySelector("#dialog_edit input[name='name']").value;
      const newSex = document.querySelector("#dialog_edit input[name='sex']").value;
      const newUsername = document.querySelector("#dialog_edit input[name='username']").value;
      // const newPassword = document.querySelector("#dialog_edit input[name='password']").value;  // Lấy giá trị password
      try {
        // Gửi yêu cầu thêm mới dữ liệu đến máy chủ
        const response = fetch('/teachers', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({
            // image: newImage,
            mssv: newMssv,
            phone: newPhone,
            name: newName,
            sex: newSex,
            username: newUsername,
            // password: newPassword
          })
        })
          .then(response => {
            if (response.ok) {
              // Nếu thêm mới thành công, làm gì đó (ví dụ: ẩn hộp thoại)
              hideDialog_edit();
              // Refresh lại trang hoặc làm gì đó để cập nhật dữ liệu hiển thị
              window.location.reload();
            } else {
              // Xử lý khi không thêm mới được dữ liệu
              console.error('Failed to add new data');
            }
          })
          .catch(error => console.error('Error:', error));
      } catch (error) {
        console.error('Error:', error);
      }
    }


  </script>


